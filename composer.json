{
    "name": "uservm/payment-api",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/rest-bundle": "^2.3",
        "incenteev/composer-parameter-handler": "^2.0",
        "mullenlowe/audi-agc5-common-bundle": "dev-develop",
        "mullenlowe/audi-agc5-plugins-bundle": "dev-develop",
        "mullenlowe/component": "dev-master",
        "mullenlowe/pay-plugin-bundle": "dev-develop",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/symfony": "3.4.*",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "codeception/codeception": "^2.4",
        "sensio/generator-bundle": "^3.0",
        "swagger-api/swagger-ui": "^3.16",
        "symfony/phpunit-bridge": "^3.0",
        "zircote/swagger-php": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "no-api": true,
            "url": "git@github.com:itagenceone/audi-agc5-common-bundle.git"
        },
        {
            "type": "vcs",
            "no-api": true,
            "url": "git@github.com:itagenceone/audi-agc5-plugins-bundle.git"
        },
        {
            "type": "vcs",
            "no-api": true,
            "url": "git@github.com:itagenceone/component.git"
        },
        {
            "type": "vcs",
            "no-api": true,
            "url": "git@github.com:itagenceone/pay-plugins.git"
        }
    ],
    "scripts": {
        "swagger-ui-assets" : "php -r \"is_dir('vendor/swagger-api/swagger-ui/dist') && !is_link('web/swagger-ui') && symlink('../vendor/swagger-api/swagger-ui/dist', 'web/swagger-ui');\"",
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts",
            "@swagger-ui-assets"
        ],
        "post-update-cmd": [
            "@symfony-scripts",
            "@swagger-ui-assets"
        ]
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "mullenlowe/payment": "source"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
